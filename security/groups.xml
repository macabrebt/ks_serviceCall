<odoo>
<record id="module_category_ks_service_call" model="ir.module.category">
        <field name="name">Service Call</field>
        <field name="sequence">10</field>
</record>
<record id="group_ks_service_call_user" model="res.groups">
    <field name="name">User</field>
    <field name="category_id" ref="module_category_ks_service_call"/>
    <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
</record>
<record id="group_ks_service_call_admin" model="res.groups">
    <field name="name">Administrator</field>
    <field name="category_id" ref="module_category_ks_service_call"/>
    <field name="implied_ids" eval="[(4, ref('group_ks_service_call_user'))]"/>
    <field name="users" eval="[(4, ref('base.user_admin'))]"/>
</record>
</odoo>